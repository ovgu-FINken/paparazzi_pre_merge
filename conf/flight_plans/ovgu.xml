<!-- DOCTYPE flight_plan SYSTEM "flight_plan.dtd" -->

<flight_plan NAME="Fink" LON0="1.27289" MAX_DIST_FROM_HOME="1000"
	GROUND_ALT="185" SECURITY_HEIGHT="25" QFU="270" ALT="250" LAT0="43.46223">
	<waypoints>
		<waypoint name="HOME" x="0.0" y="120.0" alt="250." />
	</waypoints>
	<header>
		#include "sonar/sonar_array_i2c.h"
		#include "finken_model/finken_model_actuators.h"
		#include "finken_model/finken_model_system.h"
	</header>
	<!-- <exceptions> <exception cond="(100 > sonar_values.back || 100 > sonar_values.front 
		) && ((sonar_values.right > sonar_values.back && sonar_values.left > sonar_values.back) 
		|| (sonar_values.right > sonar_values.front && sonar_values.left > sonar_values.front))" 
		deroute="failsave_pitch"/> <exception cond="(100 > sonar_values.right || 
		100 > sonar_values.left )" deroute="failsave_roll"/> </exceptions> / -->
	<blocks>
		<block name="default">
			<attitude pitch="0.0" roll="0.0" throttle="0.0" until="FALSE"
				vmode="throttle" />
		</block>

		<block name="inair">
			<attitude pitch="finken_actuators_model.alpha" roll="finken_actuators_model.beta"
				throttle="finken_actuators_model.thrust" until="FALSE" vmode="throttle" />
				<set value="1"	var="finken_system_model_control_height"/>
		</block>

		<block key="t" name="Takeoff" strip_button="Takeoff" strip_icon="takeoff.png">
			<set value="0" var="autopilot_flight_time" />
			<set value="0.5" var="finken_system_set_point.distance_z" />
			<attitude pitch="0" roll="0"
				throttle="compensate_battery_drop(0.37)" until="stage_time > 2"
				vmode="throttle" />
			<attitude pitch="0" roll="0"
				throttle="compensate_battery_drop(0.41)" until="stage_time > 2"
				vmode="throttle"/>
				<set value="1" var="finken_system_model.reset"/>
			<deroute block="inair" />
		</block>

	</blocks>
